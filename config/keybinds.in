# DF options

menu "Key bindings"

config KEYBINDS_VERSION
	string
    default "master"

config KEYBINDS
	bool
	prompt "Include keybindings"
	default y
	help
	  Select this to include alternate keybindings

config KEYBINDS_REV
	string
	prompt "Github revision of keybindings"
	default "master"
	depends on KEYBINDS
	help
	  Github revision of the keybindings from https://github.com/Lazy-Newb-Pack/LNP-shared-core

endmenu
